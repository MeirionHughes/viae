{
  "name": "viae",
  "version": "0.6.2",
  "description": "A bi-directional communication framework",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "scripts": {
    "lint": "cross-env tslint --project tsconfig.json --config tslint.json",
    "clean": "cross-env rimraf .tmp .nyc_output coverage dist",
    "build": "npm run clean && concurrently \"npm run build:es2015\" \"npm run build:commonjs\"",
    "build:es2015": "tsc --project tsconfig.build.json --rootDir src/ --outDir ./dist/es2017 --module es2015",
    "build:commonjs": "tsc --project tsconfig.build.json --rootDir src/ --outDir ./dist/commonjs --module commonjs",
    "debug": "npm run clean && cross-env tsc --project tsconfig.json --outDir ./.tmp/ --module commonjs ",
    "test": "nyc node_modules/mocha/bin/_mocha --require source-map-support/register --compilers ts:ts-node/register test/**/*.ts",
    "cover": "codecov",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viae-io/viae.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "api",
    "middleware",
    "request",
    "response",
    "websocket",
    "async-iterable",
    "rowan",
    "ws"
  ],
  "author": "Meirion Hughes <crakinshot@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viae-io/viae/issues"
  },
  "homepage": "https://github.com/viae-io/viae#readme",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.13",
    "@types/ws": "^4.0.2",
    "async-generators": "^0.2.1",
    "chai": "^4.1.2",
    "codecov": "^1.0.1",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "cross-env": "^3.2.4",
    "mocha": "^3.2.0",
    "nyc": "^11.0.3",
    "ora": "^2.1.0",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.2.0",
    "tslint": "^4.3.1",
    "typescript": "^2.4.2",
    "ws": "^5.1.1"
  },
  "dependencies": {
    "lite-event-emitter": "^1.0.2",
    "msgpack-lite": "~0.1.26",
    "path-to-regexp": "^1.7.0",
    "rowan": "1.0.0-beta.4",
    "uuid": "^3.2.1",
    "varint": "^5.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "dist/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
