{
  "name": "viae",
  "version": "0.6.0",
  "description": "bi-directional communication api-framework",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "scripts": {
    "lint": "cross-env tslint --project tsconfig.json --config tslint.json",
    "clean": "cross-env rimraf .tmp .nyc_output coverage dist",
    "build": "npm run clean && concurrently \"npm run build:es2015\" \"npm run build:commonjs\"",
    "build:es2015": "tsc --project tsconfig.build.json --rootDir src/ --outDir ./dist/es2017 --module es2015",
    "build:commonjs": "tsc --project tsconfig.build.json --rootDir src/ --outDir ./dist/commonjs --module commonjs",
    "debug": "npm run clean && cross-env tsc --project tsconfig.json --outDir ./.tmp/ --module commonjs ",
    "test": "nyc node_modules/mocha/bin/_mocha --require source-map-support/register --compilers ts:ts-node/register test/unit/**/*.ts",
    "cover": "codecov",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeirionHughes/viae.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "api",
    "middleware",
    "request",
    "response",
    "websocket",
    "async-iterable",
    "rowan",
    "ws"
  ],
  "author": "Meirion Hughes <crakinshot@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MeirionHughes/viae/issues"
  },
  "homepage": "https://github.com/MeirionHughes/viae#readme",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.13",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "cross-env": "^3.2.4",
    "mocha": "^3.2.0",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "ts-node": "^3.2.0",
    "tslint": "^4.3.1",
    "typescript": "2.4",
    "ws": "^3.0.0"
  },
  "dependencies": {
    "lite-event-emitter": "^1.0.2",
    "msgpack-lite": "^0.1.26",
    "path-to-regexp": "^1.7.0",
    "rowan": "0.7",
    "tslib": "^1.7.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "dist/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
