{
  "name": "viae",
  "version": "0.1.7",
  "description": "A bi-directional communication api framework with streaming support.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run lint",
    "test": "nyc node_modules/mocha/bin/_mocha --compilers ts:ts-node/register ./test/*.ts",
    "posttest": "npm run build",
    "cover": "codecov",
    "build": "webpack && gulp typings",
    "prepare-release": "npm test",
    "clean": "rimraf coverage .nyc_output lib",
    "lint": "tslint ./src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeirionHughes/viae.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "api",
    "rowan",
    "middleware",
    "request",
    "response",
    "websocket",
    "ws",
    "stream"
  ],
  "author": "Meirion Hughes <crakinshot@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MeirionHughes/viae/issues"
  },
  "homepage": "https://github.com/MeirionHughes/viae#readme",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.38",
    "@types/node": "^6.0.59",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.4",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "path-to-regexp": "^1.7.0",
    "rimraf": "^2.5.4",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5",
    "varint": "^5.0.0",
    "webpack": "^2.2.0",
    "ws": "^1.1.1"
  },
  "dependencies": {
    "readable-stream": "^2.2.2",
    "rowan": "^0.2.1",
    "tslib": "^1.5.0",
    "varint": "^5.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules/",
      "dist/"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
