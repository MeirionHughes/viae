{
  "name": "viae",
  "private": true,
  "devDependencies": {
    "lerna": "^3.13.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "tsc -b packages/viae packages/ws",
    "publish:release": "yarn build && lerna publish",
    "publish:canary": "yarn build && lerna publish --canary --preid beta --dist-tag beta ",
    "clean": "rimraf packages/**/dist"
  }
}
